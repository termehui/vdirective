            "name": "Mojtaba Ekramy",
            "role": "Developer"
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/termehui/vdirective"
    },
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc && vite build",
        "prepare": "npm run build",
        "test": "vitest"
    },
    "dependencies": {
        "vue": "^3.3.4"
    },
    "devDependencies": {
        "@types/node": "^20.6.2",
        "@vitejs/plugin-vue": "^4.3.4",
        "typescript": "^5.2.2",
        "vite": "^4.4.9",
