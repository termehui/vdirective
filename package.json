{
    "private": false,
    "type": "module",
    "license": "ISC",
    "version": "1.0.0",
    "name": "@termehui/vdirective",
    "description": "Directives for vue 3",
    "main": "./dist/vdirective.umd.js",
    "module": "./dist/vdirective.es.js",
    "types": "./dist/vdirective.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/vdirective.d.ts",
                "default": "./dist/vdirective.es.js"
            },
            "require": {
                "types": "./dist/vdirective.d.ts",
                "default": "./dist/vdirective.umd.js"
            }
        }
    },
    "files": [
        "dist",
        "docs"
    ],
    "keywords": [
        "vue3",
        "directive"
    ],
    "authors": [
        {
            "email": "m.ajamekramy@gmail.com",
